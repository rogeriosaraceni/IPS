---
import Layout from '@layouts/Layout.astro';
import ContentTitle from '../_partials/_content-title.astro';
import ContentFilter from '../_partials/_content-filter.astro';
import ContentNavPillars from '../_partials/_content-nav-pillars.astro';
import ContentTableMain from './_content-table-main.astro';

import '../_assets/styles.css'
---

<Layout>
    <div class="animate__animated animate__bounceInLeft">
        <ContentTitle />
        <ContentFilter />
        <ContentNavPillars />
    </div>
    <ContentTableMain />
</Layout>

<script src="../_assets/scripts.js"></script>
